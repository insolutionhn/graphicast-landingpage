          "_name": "tabs02",
          "_sourceTheme": "replym5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"tabs02 replym5\" group=\"List\" plugins=\"mbr-tabs\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <input type=\"color\" title=\"Subtitle BG\" value=\"#C6F15C\" name=\"subtitleBg\" condition=\"showSubtitle\" selected=\"\">\n        <input type=\"color\" title=\"Shadow Color\" value=\"#000000\" name=\"shadow\" condition=\"showSubtitle\" selected=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <header>Tabs</header>\n        <select title=\"Count\" name=\"bootstrapTabs\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected=\"\">4</option>\n        </select>\n        <input type=\"color\" title=\"Tabs Color\" value=\"#6a2be9\" name=\"tabs\" selected=\"\">\n        <input type=\"color\" title=\"Tabs BG\" value=\"#eadef8\" name=\"tabsBg\" selected=\"\">\n        <input type=\"color\" title=\"Active Color\" value=\"#ffffff\" name=\"active\" selected=\"\">\n        <input type=\"color\" title=\"Tabs Shadow Color\" value=\"#000000\" name=\"tabsShadow\" selected=\"\">\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showCardText\" checked=\"\">\n        <input type=\"color\" title=\"Card Bg 1\" value=\"#e3dbfc\" name=\"card_1\" selected=\"\">\n        <input type=\"color\" title=\"Card Bg 2\" value=\"#e1fa9d\" name=\"card_2\" selected=\"\">\n        <input type=\"color\" title=\"Card Bg 3\" value=\"#fbdccc\" name=\"card_3\" selected=\"\">\n        <input type=\"color\" title=\"Card Bg 4\" value=\"#f7d9fa\" name=\"card_4\" selected=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12\">\n                <div class=\"title-wrapper\" mbr-if=\"showSubtitle || showTitle || showText\">\n                    <div class=\"subtitle-wrapper\">\n                        <h3 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle, .subtitle-wrapper\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">\n                            TYPES\n                        </h3>\n                    </div>\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .tabs-wrapper\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\">\n                        <b>AI marketing</b>\n                    </h2>\n                    <div class=\"text-wrapper\">\n                        <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text, .text-wrapper\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">\n                            These assistants leverage AI and machine learning algorithms to\n                            automate tasks, analyze data, and provide insights that help marketers make informed\n                            decisions.\n                        </p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12\">\n                <div class=\"tabs-wrapper\">\n                    <ul class=\"nav nav-tabs\" role=\"tablist\">\n                        <li mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"nav-item first mbr-fonts-style\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\">\n                            <a class=\"nav-link show active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tab1\" aria-selected=\"true\">\n                                Chatbots\n                            </a>\n                        </li>\n                        <li mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 1\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\">\n                            <a class=\"nav-link mbr-fonts-style\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tab2\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                Engines\n                            </a>\n                        </li>\n                        <li mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 2\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\">\n                            <a class=\"nav-link mbr-fonts-style\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tab3\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                Analytics\n                            </a>\n                        </li>\n                        <li mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 3\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\">\n                            <a class=\"nav-link mbr-fonts-style\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tab4\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                Content\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12\">\n                <div class=\"tab-content\">\n                    <div id=\"tab1\" class=\"tab-pane in active\" role=\"tabpanel\">\n                        <div class=\"content-wrap\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                            <div class=\"text-wrap card\">\n                                <h4 class=\"item-title mbr-fonts-style\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\" mbr-theme-style=\"display-2\">\n                                    <b>Chatbots</b>\n                                </h4>\n                                <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text\" mbr-if=\"showCardText\" mbr-theme-style=\"display-4\">\n                                    Chatbots are AI-powered virtual assistants that interact with users through text or\n                                    voice conversations. In marketing, chatbots are often used for customer service,\n                                    lead generation, and personalized recommendations.\n                                </p>\n                            </div>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/image18.jpg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab2\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 1\">\n                        <div class=\"content-wrap\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                            <div class=\"text-wrap card\">\n                                <h4 class=\"item-title mbr-fonts-style\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\" mbr-theme-style=\"display-2\">\n                                    <b>Person Engines</b>\n                                </h4>\n                                <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text\" mbr-if=\"showCardText\" mbr-theme-style=\"display-4\">\n                                    Personalization engines use AI algorithms to analyze customer data and behavior,\n                                    enabling marketers to deliver tailored content, recommendations, and offers to\n                                    individual users.\n                                </p>\n                            </div>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/image15.jpg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab3\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 2\">\n                        <div class=\"content-wrap\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                            <div class=\"text-wrap card\">\n                                <h4 class=\"item-title mbr-fonts-style\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\" mbr-theme-style=\"display-2\">\n                                    <b>Analytics Platforms</b>\n                                </h4>\n                                <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text\" mbr-if=\"showCardText\" mbr-theme-style=\"display-4\">\n                                    Predictive analytics platforms utilize AI and machine learning to forecast future\n                                    trends, customer behavior, and marketing outcomes based on historical data.\n                                </p>\n                            </div>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/image14.jpg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab4\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 3\">\n                        <div class=\"content-wrap\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                            <div class=\"text-wrap card\">\n                                <h4 class=\"item-title mbr-fonts-style\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\" mbr-theme-style=\"display-2\">\n                                    <b>Content Creation</b>\n                                </h4>\n                                <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text\" mbr-if=\"showCardText\" mbr-theme-style=\"display-4\">\n                                    AI-powered content creation tools use natural language generation (NLG) algorithms\n                                    to automate the process of generating written content, such as blog posts.\n                                </p>\n                            </div>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/image9.jpg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n",
