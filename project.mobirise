              "justify-content": "center",
              "@media (max-width: 992px)": {
                "justify-content": "flex-start"
              },
              "img": {
                "width": "50px",
                "height": "50px",
                "border-radius": "100%",
                "object-fit": "contain"
              }
            },
            ".nav-wrapper": {
              "& when (@showBorder)": {
                "padding": "48px 0",
                "@media (max-width: 992px)": {
                  "padding": "32px 0"
                }
              },
              ".list": {
                "padding": "0",
                "margin": "0",
                "left": "0",
                "list-style-type": "none",
                "display": "inline-flex",
                "flex-wrap": "wrap",
                "@media (max-width: 768px)": {
                  "display": "block"
                },
                ".item-wrap": {
                  "margin": "8px 0 8px 24px",
                  "position": "relative",
                  "&:hover, &:focus": {
                    "&::before": {
                      "width": "100%"
                    }
                  },
                  "&::before": {
                    "content": "''",
                    "position": "absolute",
                    "bottom": "0",
                    "left": "0",
                    "width": "0",
                    "height": "1px",
                    "background-color": "@active",
                    "transition": "all 0.3s ease-in-out"
                  },
