          "_name": "pricing01",
          "_sourceTheme": "replym5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"pricing01 replym5\" group=\"Pricing\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Card Description\" name=\"showDesc\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#6a2be9\" name=\"border\" condition=\"showTitle || showDesc\" selected>\n        <input type=\"checkbox\" title=\"Card Buttons\" name=\"showButtons\" checked>\n        <input type=\"checkbox\" title=\"Card List\" name=\"showList\" checked>\n        <input type=\"color\" title=\"Marker Color\" value=\"#ffffff\" name=\"marker\" condition=\"showList\" selected>\n        <input type=\"color\" title=\"Marker BG\" value=\"#6a2be9\" name=\"markerBg\" condition=\"showList\" selected>\n        <input type=\"color\" title=\"Main Border Color\" value=\"#bdc5cf\" name=\"borderMain\" selected>\n        <input type=\"color\" title=\"Active Color\" value=\"#6a2be9\" name=\"active\" selected>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-3 col-md-6 item features-without-image\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <div class=\"title-wrapper\" mbr-if=\"showTitle || showDesc\">\n                            <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\" mbr-if=\"showTitle\">\n                                <b>Starter Plan</b>\n                            </h4>\n                            <p class=\"item-desc mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-desc\" mbr-if=\"showDesc\">\n                                Per month\n                            </p>\n                        </div>\n                        <p class=\"item-price mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-price, .mbr-section-btn\">\n                            <b>$5</b></p>\n                        <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\">\n                            <a class=\"btn btn-primary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                                Buy now\n                            </a>\n                        </div>\n                        <div class=\"list-wrapper\" mbr-if=\"showList\">\n                            <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap, .list-wrapper\" data-multiline mbr-article>\n                                <li class=\"item-wrap\">Small businesses</li>\n                                <li class=\"item-wrap\">Startups</li>\n                                <li class=\"item-wrap\">Basic marketing needs</li>\n                                <li class=\"item-wrap\">Social media</li>\n                                <li class=\"item-wrap\">Basic data analytics</li>\n                                <li class=\"item-wrap\">1 Email marketing</li>\n                                <li class=\"item-wrap\">Certain number</li>\n                                <li class=\"item-wrap\">Email support</li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-3 col-md-6 item features-without-image\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <div class=\"title-wrapper\" mbr-if=\"showTitle || showDesc\">\n                            <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\" mbr-if=\"showTitle\">\n                                <b>Standard Plan</b>\n                            </h4>\n                            <p class=\"item-desc mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-desc\" mbr-if=\"showDesc\">\n                                Per month\n                            </p>\n                        </div>\n                        <p class=\"item-price mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-price, .mbr-section-btn\">\n                            <b>$259</b>\n                        </p>\n                        <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\">\n                            <a class=\"btn btn-primary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                                Buy now\n                            </a>\n                        </div>\n                        <div class=\"list-wrapper\" mbr-if=\"showList\">\n                            <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap, .list-wrapper\" data-multiline mbr-article>\n                                <li class=\"item-wrap\">Growing businesses</li>\n                                <li class=\"item-wrap\">Mid-sized companies</li>\n                                <li class=\"item-wrap\">Online presence</li>\n                                <li class=\"item-wrap\">AI-powered insights</li>\n                                <li class=\"item-wrap\">Multi-channel</li>\n                                <li class=\"item-wrap\">Management</li>\n                                <li class=\"item-wrap\">A/B testing</li>\n                                <li class=\"item-wrap\">Higher limits</li>\n                                <li class=\"item-wrap\">Email sends</li>\n                                <li class=\"item-wrap\">Priority email</li>\n                                <li class=\"item-wrap\">Chat support</li>\n                            </ul>\n                        </div>\n                    </div>\n                