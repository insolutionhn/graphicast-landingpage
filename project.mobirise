          "_name": "slider01",
          "_sourceTheme": "replym5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"slider01 mbr-embla replym5\" group=\"Sliders &amp; Galleries\" plugins=\"embla\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showTitle_1\" title=\"Title 1\" checked=\"\">\n        <input type=\"checkbox\" name=\"showTitle_2\" title=\"Title 2\" checked=\"\">\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"1\" max=\"10\" value=\"5\" step=\"1\" condition=\"autoplay\">\n        <input type=\"checkbox\" title=\"Loop\" name=\"loop\" checked=\"\">\n        <input type=\"checkbox\" title=\"Draggable\" name=\"draggable\" checked=\"\">\n        <input type=\"checkbox\" name=\"arrows\" title=\"Arrows\">\n        <input type=\"checkbox\" name=\"arrowsMobile\" title=\"Mobile Arrows\" condition=\"arrows\" checked=\"\">\n        <header>Cards</header>\n        <input type=\"range\" name=\"gap\" title=\"Gap\" min=\"-1\" max=\"10\" step=\"1\" value=\"-1\">\n        <input type=\"checkbox\" name=\"showText\" title=\"Card Text\" checked=\"\">\n        <input type=\"checkbox\" name=\"showName\" title=\"Card Name\" checked=\"\">\n        <input type=\"checkbox\" name=\"showRole\" title=\"Card Role\" checked=\"\">\n        <input type=\"color\" name=\"card_1\" title=\"Card BG 1\" value=\"#C6F15C\">\n        <input type=\"color\" name=\"card_2\" title=\"Card BG 2\" value=\"#b0a0ff\">\n        <input type=\"color\" name=\"card_3\" title=\"Card BG 3\" value=\"#f0a2fd\">\n        <input type=\"color\" name=\"card_4\" title=\"Card BG 4\" value=\"#FEB591\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\" required=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-9\">\n                <h2 class=\"mbr-section-title_1 mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle_1\" data-app-selector=\".mbr-section-title_1\">\n                    <b>Reviews</b>\n                </h2>\n            </div>\n            <div class=\"col-12\">\n                <div class=\"embla\" data-loop=\"{{loop}}\" data-auto-play=\"{{autoplay}}\" data-auto-play-interval=\"{{interval}}\" data-draggable=\"{{draggable}}\" data-skip-snaps=\"true\" data-align=\"center\" data-contain-scroll=\"trimSnaps\">\n                    <div class=\"embla__viewport\">\n                        <div class=\"embla__container\">\n                            <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-content\">\n                                            <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\" mbr-if=\"showText\">\n                                                \"As an e-commerce manager, I've seen firsthand the impact of online sales\n                                                promotion on our bottom line.\"\n                                            </p>\n                                        </div>\n                                        <div class=\"image-wrapper\">\n                                            <div class=\"item-img\">\n                                                <div class=\"image-wrap\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image6.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"person-wrap\" mbr-if=\"showName || showRole\">\n                                                    <p class=\"item-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-name, .image-wrapper\" mbr-if=\"showName\">\n                                                        <b>James Reynolds</b>\n                                                    </p>\n                                                    <p class=\"item-role mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-role, .image-wrapper\" mbr-if=\"showRole\">\n                                                        E-commerce Manager\n                                                    </p>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-content\">\n                                            <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\" mbr-if=\"showText\">\n                                                \"In the competitive tech industry, engaging with our audience and driving\n                                                conversions is paramount.\"\n                                            </p>\n                                        </div>\n                                        <div class=\"image-wrapper\">\n                                            <div class=\"item-img\">\n                                                <div class=\"image-wrap\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image5.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"person-wrap\" mbr-if=\"showName || showRole\">\n                                                    <p class=\"item-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-name, .image-wrapper\" mbr-if=\"showName\">\n                                                        <b>Sarah Patel</b>\n                                                    </p>\n                                                    <p class=\"item-role mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-role, .image-wrapper\" mbr-if=\"showRole\">\n                                                        Marketing Director\n                                                    </p>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-content\">\n                                            <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\" mbr-if=\"showText\">\n                                                \"As a small business owner, competing with larger retailers can be\n                                                challenging.\"\n                                            </p>\n                                        </div>\n                                        <div class=\"image-wrapper\">\n                                            <div class=\"item-img\">\n                                                <div class=\"image-wrap\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image13.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"person-wrap\" mbr-if=\"showName || showRole\">\n                                                    <p class=\"item-name mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-name, .image-wrapper\" mbr-if=\"showName\">\n                                                        <b>Michael Thompson</b>\n                                                    </p>\n                                                    <p class=\"item-role mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-role, .image-wrapper\" mbr-if=\"showRole\">\n                                                        Business Owner\n                                                    </p>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-content\">\n                                            <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\" mbr-if=\"showText\">\n                                                \"Working in the fashion industry, staying ahead of trends and reaching\n                                                our target audience is essential.\"\n                                            </p>\n                                        </div>\n                                        <div class=\"image-wrapper\">\n                                            <div class=\"item-img\">\n                                                <div class=\"image-wrap\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image7.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"person-wrap\" mbr-if=\"showName || showRole\">\n               